	NAME=Реализации алгоритмов____________________Длинна массива / строки strlen(arrstr[0]);n=strlen((unsigned char*)c)/sizeof(int);int n = sizeof(massiv) / sizeof(massiv[0]);#define STRLEN(s) ({ register typeof((s)[0]) _start = (s); \ register typeof((s)[0]) *_s = _start; \ while (_s++); \ (size_t)(_s - _start - 1); })size_t strlen (const char *str) { register const char *s = str; while(*str++); return str - s - 1; }void my_strlen(const char *str, size_t *len){        while(*(str++)) (*len)++;}//Через рекурсиюsize_t str_len (const char *str){    return (*str) ? str_len(++str) + 1 : 0;}int my_strlen(const char *str){    int len;    for (len = 0; ; ++str, ++len)     {     	if (!*str) return len;     }}void my_strlen(int* n, const char* s) {    while (s[++(*n)]);}int main() {    const char *s = "hello world";    int n = 0;    my_strlen(&n, s);    printf ("%i\n", n);    return 0;}____________________Считыаем входные параметры if (sscanf(argv[i], "%d", &curr) != 0)             {                 printf("%d\n",curr);             }Отлов и обработка ошибок assert (! ferror (stdin));_____________________ССЫЛКИ и УКАЗАТЕЛИСсылка предваряется оператором изъятия адреса &int func (int& ); //прототип функцииint func (int& param){;}//внимание на передачу параметра в mainint a=12;func (a);При использовании указателей в параметре мы передаем в качестве параметра адрес переменной a.int func (int* ); // прототип int func (int* param) { ; } // внимание на вызов функции в main int a=21; func (&a);___________________БИНАРНЫЙ ПОИСК int binary_search(int A[], int key, int imin, int imax){  // test if array is empty  if (imax <= imin)    return -1;  else    {      int imid = (imin + imax)/2;      if (A[imid] > key)        return binary_search(A, key, imin, imid);      else if (A[imid] < key)        return binary_search(A, key, imid, imax);      else        return imid;    }}int binary_search(int A[], int key, int imin, int imax){  while (imin <= imax)    {      int imid = (imin + imax)/2;      if (A[imid] == key)        return imid;      else if (A[imid] < key)        imin = imid + 1;      else                imax = imid - 1;    }  return -1;}_____________________
